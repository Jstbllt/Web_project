<!-- views/cpus_list.ejs -->
<%- include("partial_headers.ejs"); %>


<section class="bg-black text-white p-1">
    <h2 class="ms-2">List of Cpu</h2>
</section>

<% if (flashMessage) { %>
    <p style='color:red'><%= flashMessage %></p>
<% } %>

<section class="p-3">

    <table class="table table-striped table-hover table-responsive-sm border border-dark shadow align-middle">
        <thead>
            <tr class="table-active fw-bold">
                <td>ID</td>
                <td>Brand</td>
                <td>Model</td>
                <td>Base Frequency</td>
                <td>Boost Frequency</td>
                <td>Cores</td>
                <td>Show</td>
            </tr>
        </thead>
        <tbody>
        <% for (var c of cpus) { %>
            <tr>
                <td><%= c.cpu_id %></td>
                <td><%= c.brand_name %></td>
                <td><%= c.cpu_model %></td>
                <td><%= c.cpu_basefrequency %></td>
                <td><%if(c.cpu_boostfrequency==null){%>
                        NONE<%}%>
                    <%= c.cpu_boostfrequency %></td>

                <td><%= c.cpu_cores %></td>
                <td><button type="button" class="btn btn-outline-dark"><a href='/cpus/show/<%= c.cpu_id %>'>Show</a></button></td>
            </tr>
        <% } %>
        </tbody>
</table>
</section>
<%- include("partial_footer.ejs"); %>